{{>layouts/headers}}
<!-- contents -->
<form class="container" action="/member/{{editMember.id}}/edit/saved" method="post">
    <div class="mb-3">
        <label class="form-label">이메일</label>
        <input type="email" class="form-control" name="email" value="{{editMember.email}}">
    </div>
    <div class="mb-3">
        <label class="form-label">비밀번호</label>
        <input type="password" class="form-control" name="password" value="{{editMember.password}}">
    </div>
    <button type="submit" class="btn btn-primary">수정완료</button>
    <a href="/member/{{editMember.id}}">Back</a>
</form>

{{>layouts/footers}}
